MembershipFormSubmit = (e) => {\n    e.preventDefault();\n\n    fetch(\"https://cewa-backend.herokuapp.com/membership\", {\n      method: \"POST\",\n      body: {\n        name,\n        email,\n        phone,\n        dob,\n        sex,\n        profession,\n        city,\n        country,\n      },\n    });\n  };\n\n  return (\n    <div className=\"joinUs\">\n      <div className=\"joinUs_left\">\n        <div className=\"overlay\"></div>\n        <img\n          src=\"https://images.unsplash.com/photo-1488521787991-ed7bbaae773c?ixid=MXwxMjA3fDB8MHxzZWFyY2h8MXx8cG9vciUyMGNoaWxkcmVufGVufDB8fDB8&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60\"\n          alt=\"Poor children\"\n        />\n      </div>\n      <div className=\"joinUs_right\">\n        <h1 className=\"membershipForm_header\">Become a member</h1>\n        <p className=\"membershipForm_subheader\">Be the change</p>\n        <form className=\"membershipForm\">\n          <div className=\"membershipForm_field\">\n            <label for=\"name\">Name</label>\n            <input\n              type=\"text\"\n              name=\"name\"\n              value={name}\n              onChange={(e) => {\n                setName(e.target.value);\n              }}\n            />\n          </div>\n\n          <div className=\"membershipForm_field\">\n            <label for=\"email\">Email</label>\n            <input\n              type=\"text\"\n              name=\"email\"\n              value={email}\n              onChange={(e) => {\n                setEmail(e.target.value);\n              }}\n            />\n          </div>\n\n          <div className=\"membershipForm_field\">\n            <label for=\"phone\">Phone</label>\n            <input\n              type=\"text\"\n              name=\"phone\"\n              value={phone}\n              onChange={(e) => {\n                setPhone(e.target.value);\n              }}\n            />\n          </div>\n\n          <div className=\"membershipForm_field\">\n            <label for=\"dob\">Date of Birth</label>\n            <input\n              type=\"date\"\n              name=\"dob\"\n              value={dob}\n              onChange={(e) => {\n                setDOB(e.target.value);\n              }}\n            />\n          </div>\n\n          <div className=\"membershipForm_field\">\n            <label for=\"sex\">Sex</label>\n            <select\n              name=\"sex\"\n              onChange={(e) => setSex(e.target.value)}\n              value={sex}\n            >\n              <option value=\"null\">--Select--</option>\n              <option value=\"M\">M</option>\n              <option value=\"F\">F</option>\n              <option value=\"Other\">Other</option>\n            </select>\n          </div>\n\n          <div className=\"membershipForm_field\">\n            <label for=\"profession\">Profession</label>\n            <input\n              type=\"text\"\n              name=\"profession\"\n              value={profession}\n              onChange={(e) => {\n                setProfession(e.target.value);\n              }}\n            />\n          </div>\n\n          <div className=\"membershipForm_field\">\n            <label for=\"city\">City</label>\n            <input\n              type=\"text\"\n              name=\"city\"\n              value={city}\n              onChange={(e) => {\n                setCity(e.target.value);\n              }}\n            />\n          </div>\n\n          <div className=\"membershipForm_field\">\n            <label for=\"country\">Country</label>\n            <input\n              type=\"text\"\n              name=\"country\"\n              value={country}\n              onChange={(e) => {\n                setCountry(e.target.value);\n              }}\n            />\n          </div>\n\n          <Button onClick={handleMembershipFormSubmit}>Submit</Button>\n        </form>\n      </div>\n    </div>\n  );\n}\n\nexport default JoinUs;\n","/home/beast/Desktop/CEW